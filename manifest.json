{
  "name": "MySymptoms-MS",
  "version": "1.03",
  "manifest_version": 2,
  "description": "Daily popup for tracking various Relapsing-Remitting MS symptoms.",
  "icons": {
    "128": "ms_icon_resized.png"
  },
  "browser_action": {
    "default_title": "How are you feeling today? Rate your symptoms!",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{ "js": ["content.js"], "matches": ["<all_urls>"] }],
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "identity",
    "https://*.googleapis.com/*",
    "http://localhost:8080/api/routes"
  ],
  "oauth2": {
    "client_id": "48454209769-ijq5aai16irvt8o7c808kv7tmdsgf73m.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/spreadsheets"]
  },
  "key": ""
}
